{
  "name": "booking-for-pets",
  "version": "0.1.0",
  "description": "A React.js project using Typescript, with which you can book a hotel room for your pet",
  "scripts": {
    "build": "npm run clean-dist && webpack --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet",
    "start": "npm run start-dev",
    "start-dev": "webpack serve --config=configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json",
    "server": "ts-node ./src/server/server.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
